<form id="formProduct">
    <h2>Tés del Sur</h2>
    <input class="controls" type="text" name="title" id="title" placeholder="Ingresar Nombre del Té" required>
    <input class="controls" type="text" name="description" id="description" placeholder="Ingresar una descripción" required>
    <input class="controls" type="number" name="stock" id="stock" placeholder="Ingresar stock" required>
   <input class="controls" type="text" name="thumbnail" id="thumbnail" placeholder="Ingresar imagen url" required>
    <input class="controls" type="text" name="category" id="category" placeholder="Ingresar categoría" required>
    <input class="controls" type="number" name="price" id="price" placeholder="Ingresar precio" required>
   <input class="controls" type="text" name="code" id="code" placeholder="Ingresar code" required>
    <button class="botons" type="submit" id="add-btn" >Registrar </button>
    <h2> Para eliminar un producto:</h2>
    <input class="controls" type="text" name="id-prod" id="id-prod" placeholder="Ingrese ID" >
   <button class="botons" type="button" id="delete-btn">Eliminar</button>

</form>

<div id="contenedor">
</div>

